#2019年4月24日python第一次作业
for i in range(7)：
    if i == 0:
        print("周一喝酒吃烧烤")
    elif i == 1:
        print("周二吃凉皮")
    elif i == 2：
        print("周三吃排骨饭")
    elif i == 3：
        print("周四吃番茄鸡蛋面")
    elif i == 4：
        print("周五吃牛腩面")
    elif i == 5：
        print("周六喝粥")
    elif i == 6：
        print("周日随便吃点")
    
    